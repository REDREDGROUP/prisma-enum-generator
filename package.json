{
  "name": "@redredgroup/prisma-enum-generator",
  "version": "1.0.1",
  "description": "simple prisma enum generator",
  "main": "dist/index.js",
  "scripts": {
    "watch": "tsc -w",
    "start": "node dist/index.js",
    "clean": "rimraf -rf ./dist",
    "build": "yarn clean && tsc",
    "release": "standard-version"
  },
  "keywords": [
    "prisma plugins",
    "prisma typescript",
    "enum value generator",
    "prisma enum",
    "prisma generator"
  ],
  "author": "K- SEI HOON",
  "license": "MIT",
  "release": {
    "branches": [
      "main"
    ]
  },
  "homepage": "https://github.com/REDREDGROUP/prisma-enum-generator#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/REDREDGROUP/prisma-enum-generator.git"
  },
  "publishConfig": {
    "access": "public",
    "branches": [
      "main"
    ]
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@prisma/client": "^3.15.2",
    "@prisma/generator-helper": "^3.15.2",
    "pg": "^8.7.3",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/pg": "^8.6.5",
    "@types/prettier": "^2.6.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prisma": "^3.15.2",
    "standard-version": "^9.5.0"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
